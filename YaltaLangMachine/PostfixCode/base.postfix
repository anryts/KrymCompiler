.target: Postfix Machine
.version: 0.2

.vars(
   a     int        
   b     float      
)

.labels(
   start 10 
)

.constants(
   5     int        
   2.5   float      
)

.code(
   a     l-val 
   5     int   
   :=    assign_op
   b     l-val 
   2.5   float 
   :=    assign_op
   b     r-val 
   1.5   float 
   +     add_op
   :=    assign_op
   b     r-val 
   out_op
)
